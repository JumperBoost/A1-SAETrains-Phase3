<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<fx:root prefWidth="340.0" stylesheets="@../css/style.css" type="VBox" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1">
    <FlowPane fx:id="cartesReserve" alignment="CENTER" hgap="5.0" vgap="5.0" />
    <VBox styleClass="cardsBox">
        <HBox fx:id="cartesEcartees" alignment="CENTER">
            <StackPane>
                <ImageView>
                    <Image url="@../images/cartes/horaires_estivaux.jpg" />
                    <fitWidth>75</fitWidth>
                    <fitHeight>105</fitHeight>
                </ImageView>
            </StackPane>
            <VBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
        </HBox>
        <HBox alignment="CENTER">
            <ImageView>
                <Image url="@../images/boutons/defausse.png" />
                <fitWidth>24</fitWidth>
                <fitHeight>24</fitHeight>
                <HBox.margin>
                    <Insets right="5.0" />
                </HBox.margin>
            </ImageView>
            <Label text="Écartées" />
            <VBox.margin>
                <Insets left="5.0" />
            </VBox.margin>
        </HBox>
    </VBox>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</fx:root>
